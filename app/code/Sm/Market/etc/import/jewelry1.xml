<?xml version="1.0"?>
<root>
    <config>
        <web>
            <default>
                <cms_home_page>home-demo-21</cms_home_page>
                <cms_no_route>no-route</cms_no_route>
            </default>
        </web>
        <cms>
            <wysiwyg>
                <enabled>hidden</enabled>
            </wysiwyg>
        </cms>
        <bundleimage>
            <general>
                <show_quickview>1</show_quickview>
                <show_addtocart>1</show_addtocart>
                <show_addtowishlist>1</show_addtowishlist>
                <show_addtocompare>1</show_addtocompare>
                <show_rating>1</show_rating>
                <show_viewconfig_button>1</show_viewconfig_button>
                <show_print>1</show_print>
                <no_select_image/>
            </general>
        </bundleimage>
        <cartquickpro>
            <general>
                <isenabled>1</isenabled>
                <select_type>both</select_type>
                <product_container>.product-item-info</product_container>
                <button_container>.product-item-details,.product-item-info .quickview-container</button_container>
                <label_button>Quick View</label_button>
                <countdown_number>5</countdown_number>
            </general>
        </cartquickpro>
        <categories>
            <general>
                <isenabled>1</isenabled>
                <title_module>SM Categories</title_module>
            </general>
            <source>
                <select_category/>
                <limitation>5</limitation>
            </source>
        </categories>
        <filterproducts>
            <general>
                <enable>1</enable>
                <title_module>New Products</title_module>
                <type_show>simple</type_show>
            </general>
            <source>
                <display_countdown>0</display_countdown>
                <product_source>lastest_products</product_source>
                <date_to/>
                <select_category/>
                <product_order_by>name</product_order_by>
                <product_order_dir>ASC</product_order_dir>
                <product_limitation>5</product_limitation>
            </source>
        </filterproducts>
        <listingtabs>
            <general>
                <isactive>1</isactive>
                <title>SM Listing Tabs</title>
                <type_show>loadmore</type_show>
                <type_listing>all</type_listing>
                <under_price>4.99</under_price>
                <display_countdown>0</display_countdown>
            </general>
            <source>
                <type_filter>categories</type_filter>
                <field_tabs>name</field_tabs>
                <category_tabs/>
                <category_select/>
                <order_by>name</order_by>
                <order_dir>ASC</order_dir>
                <limitation>8</limitation>
            </source>
            <source_options>
                <type_filter>categories</type_filter>
                <order_by>name</order_by>
                <order_dir>DESC</order_dir>
                <limitation>10</limitation>
            </source_options>
        </listingtabs>
        <market>
            <theme_layout>
                <direction_rtl>0</direction_rtl>
                <layout_header>
                    <header_style>header-21</header_style>
                </layout_header>
                <layout_footer>
                    <footer_style>footer-14</footer_style>
                </layout_footer>
                <layout_product>
                    <product_style>product-7</product_style>
                </layout_product>
            </theme_layout>
            <theme_install>
                <overwrite_blocks>0</overwrite_blocks>
                <overwrite_pages>0</overwrite_pages>
            </theme_install>
            <mobile_theme_layout>
                <home_page_mobile>home-demo-21</home_page_mobile>
                <logomobile_width>100</logomobile_width>
                <logomobile_height>35</logomobile_height>
                <logo_mobile>default/logo.png</logo_mobile>
            </mobile_theme_layout>
            <product_information>
                <buy_sm_theme>
                    <show_buytheme>0</show_buytheme>
                    <buy_theme_name>BUY SM MARKET</buy_theme_name>
                    <buy_theme_url>
                        https://themeforest.net/item/market-premium-responsive-magento-2-19-store-theme/8945695
                    </buy_theme_url>
                </buy_sm_theme>
            </product_information>
        </market>
        <megamenu>
            <general>
                <isenabled>1</isenabled>
                <group_id>2</group_id>
                <theme>1</theme>
                <effect>1</effect>
                <effect_duration>800</effect_duration>
                <start_level>1</start_level>
                <end_level>5</end_level>
                <limit>13</limit>
            </general>
        </megamenu>
        <searchbox>
            <general>
                <root_catalog>2</root_catalog>
                <max_level>3</max_level>
                <enable_searchpro>0</enable_searchpro>
            </general>
        </searchbox>
        <sizechart>
            <general>
                <enable>1</enable>
                <title_sizechart>Size Chart</title_sizechart>
                <sizechart_code>size</sizechart_code>
                <size_chart_position>options</size_chart_position>
                <sizechart_icon>default/size-chart-icon.png</sizechart_icon>
            </general>
        </sizechart>
        <themecore>
            <general>
                <primary_color>
                    <theme_color>BE9969</theme_color>
                    <text_on_theme_color>FFFFFF</text_on_theme_color>
                    <link_on_theme_color>FFFFFF</link_on_theme_color>
                    <linkhover_on_theme_color>FFFFFF</linkhover_on_theme_color>
                </primary_color>
                <bodyfont_group>
                    <body_font>google_font</body_font>
                    <body_googlefont_link>&lt;link href="https://fonts.googleapis.com/css?family=Open+Sans:400,600,700"
                        rel="stylesheet"&gt;
                    </body_googlefont_link>
                    <body_googlefont_family>font-family: 'Open Sans', sans-serif;</body_googlefont_family>
                    <body_googlefont_weight>400</body_googlefont_weight>
                    <font_size>13px</font_size>
                    <body_link_color>444444</body_link_color>
                    <body_link_hover_color>BE9969</body_link_hover_color>
                    <body_text_color>666666</body_text_color>
                </bodyfont_group>
                <background_group>
                    <body_background_color>FFFFFF</body_background_color>
                    <use_background_image>0</use_background_image>
                    <body_background_image/>
                    <background_repeat>repeat</background_repeat>
                    <background_position>left top</background_position>
                </background_group>
                <element_googlefont_group>
                    <use_element_googlefont>0</use_element_googlefont>
                    <element_googlefont_link/>
                    <element_googlefont_family/>
                    <element_googlefont_weight/>
                    <google_font_targets/>
                </element_googlefont_group>
                <navigation_group>
                    <menu_style>megamenu</menu_style>
                    <menu_ontop>0</menu_ontop>
                </navigation_group>
                <mobile_layout>
                    <mobile_break_point>1024</mobile_break_point>
                    <logo_mobile>default/logo-mobile_1.png</logo_mobile>
                    <logomobile_width>129</logomobile_width>
                    <logomobile_height>32</logomobile_height>
                </mobile_layout>
            </general>
            <theme_layout>
                <max_width>1690px</max_width>
                <use_boxed_layout>0</use_boxed_layout>
                <boxed_background_color>FFFFFF</boxed_background_color>
            </theme_layout>
            <product_listing>
                <category_one_column>
                    <one_column_1921>4</one_column_1921>
                    <one_column_1681_1920>4</one_column_1681_1920>
                    <one_column_1441_1680>4</one_column_1441_1680>
                    <one_column_1200_1440>4</one_column_1200_1440>
                    <one_column_992_1199>3</one_column_992_1199>
                    <one_column_768_991>3</one_column_768_991>
                    <one_column_481_767>2</one_column_481_767>
                    <one_column_0_480>2</one_column_0_480>
                </category_one_column>
                <category_two_column>
                    <two_column_1921>4</two_column_1921>
                    <two_column_1681_1920>4</two_column_1681_1920>
                    <two_column_1441_1680>3</two_column_1441_1680>
                    <two_column_1200_1440>3</two_column_1200_1440>
                    <two_column_992_1199>3</two_column_992_1199>
                    <two_column_768_991>2</two_column_768_991>
                    <two_column_481_767>2</two_column_481_767>
                    <two_column_0_480>2</two_column_0_480>
                </category_two_column>
                <category_three_column>
                    <three_column_1921>2</three_column_1921>
                    <three_column_1681_1920>2</three_column_1681_1920>
                    <three_column_1441_1680>2</three_column_1441_1680>
                    <three_column_1200_1440>2</three_column_1200_1440>
                    <three_column_992_1199>2</three_column_992_1199>
                    <three_column_768_991>2</three_column_768_991>
                    <three_column_481_767>2</three_column_481_767>
                    <three_column_0_480>2</three_column_0_480>
                </category_three_column>
            </product_listing>
            <product_detail>
                <image_box_group>
                    <thumbs_style>horizontal</thumbs_style>
                    <image_loop>1</image_loop>
                    <image_keyboard>1</image_keyboard>
                    <image_navigation>1</image_navigation>
                    <image_fullscreen>1</image_fullscreen>
                    <thumbs_width>98</thumbs_width>
                    <thumbs_height>98</thumbs_height>
                    <thumbs_navigation>1</thumbs_navigation>
                    <thumbs_margin>10</thumbs_margin>
                    <thumbs_borderwidth>1</thumbs_borderwidth>
                    <breakpoints_width>1200</breakpoints_width>
                    <use_zoom_image>1</use_zoom_image>
                    <zoom_width>420</zoom_width>
                </image_box_group>
                <related_upsell_group>
                    <show_related>1</show_related>
                    <show_upsell>1</show_upsell>
                    <slider_control>data-autoplay="false" data-nav="true" data-dots="false" data-screen0="2"
                        data-screen481="2" data-screen768="3" data-screen992="4" data-screen1200="4" data-screen1441="5"
                        data-screen1681="6" data-screen1920="6" data-margin="30" data-autoplayhoverpause="true"
                        data-loop="false" data-center="false" data-stagepadding="0" data-mousedrag="true"
                        data-touchdrag="true"
                    </slider_control>
                </related_upsell_group>
                <tabs_group>
                    <tab_style>default</tab_style>
                    <show_custom_tab>1</show_custom_tab>
                    <custom_tab_name>Custom Tabs</custom_tab_name>
                    <customtab_content>Write your content here...</customtab_content>
                </tabs_group>
                <sticky_addtocart_tabs>
                    <show_sticky_addtocart>1</show_sticky_addtocart>
                    <show_sticky_tabs>1</show_sticky_tabs>
                    <enable_sticky_bar>1</enable_sticky_bar>
                    <position_bottom>0</position_bottom>
                </sticky_addtocart_tabs>
                <buynow_button>
                    <enable_buynow>1</enable_buynow>
                </buynow_button>
            </product_detail>
            <advanced>
                <backtotop_group>
                    <show_backtotop>1</show_backtotop>
                </backtotop_group>
                <product_group>
                    <show_addtocart_button>1</show_addtocart_button>
                    <show_wishlist_button>1</show_wishlist_button>
                    <show_compare_button>1</show_compare_button>
                    <show_newlabel>1</show_newlabel>
                    <show_salelabel>1</show_salelabel>
                    <show_discount>1</show_discount>
                    <show_imagehover>1</show_imagehover>
                </product_group>
                <newsletter_group>
                    <show_newsletter_popup>1</show_newsletter_popup>
                </newsletter_group>
                <copyright_group>
                    <custom_copyright>1</custom_copyright>
                    <copyright_content>SM Market &#xA9; 2021&#xD;
                        Demo Store. All Rights Reserved. Designed by&#xD;
                        &lt;a title="MagenTech.Com" target="_blank" href="http://www.MagenTech.Com"&gt;MagenTech.Com&lt;/a&gt;
                    </copyright_content>
                </copyright_group>
                <developer_group>
                    <enable_custom_css>0</enable_custom_css>
                    <custom_css/>
                    <enable_custom_js>0</enable_custom_js>
                    <custom_js/>
                </developer_group>
                <lazyload_group>
                    <enable_ladyloading>1</enable_ladyloading>
                    <enable_page_ladyloading>0</enable_page_ladyloading>
                    <page_loading_icon>default/cup-loading.gif</page_loading_icon>
                </lazyload_group>
            </advanced>
        </themecore>
    </config>
</root>
